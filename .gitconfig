[user]
	name = JD
	email = john.dennis@skykick.com
[core]
	symlinks = false
	preloadindex = true
	fscache = true
	editor = vim
	autocrlf = true
[color]
	diff = auto
	status = auto
	branch = auto
	interactive = true
[push]
	default = simple
[gc]
	auto = 256
[diff]
    tool = bc4
[difftool]
    prompt = false
[difftool "bc4"]
    cmd =  \"c:/program files (x86)/beyond compare 4/bcomp.exe\" "$(cygpath -w $LOCAL)" "$REMOTE"
[merge]
    tool = bc4
[mergetool]
    prompt = false
[mergetool "bc4"]
    #trustExitCode = true
    cmd = \"c:/program files (x86)/beyond compare 4/bcomp.exe\" "$LOCAL" "$REMOTE" "$BASE" "$MERGED"


